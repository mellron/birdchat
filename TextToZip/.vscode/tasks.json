{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-linux-exe",
      "type": "process",
      "command": "dotnet",
      "args": [
        "publish",
        "-c",
        "Debug",
        "-r",
        "linux-x64",
        "--output",
        "${workspaceFolder}/bin/Debug/net9.0/linux-x64/"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
